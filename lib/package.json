{
  "name": "e-prim",
  "description": "A set of tools to implement a design system on top of @emotion/css and @emotion/react as well as add some primitive components to make prototyping UI faster and easier",
  "version": "1.7.0",
  "author": "Daniel Stoyanoff <dannystoyanoff@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://dstoyanoff.github.io/e-prim",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "dev": "yarn --cwd ./docs dev",
    "example": "yarn --cwd ./example install && yarn --cwd ./example dev",
    "build": "tsup",
    "test": "jest",
    "bundlewatch": "bundlewatch --config .bundlewatch.config.js",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "peerDependencies": {
    "@emotion/react": ">11",
    "react": ">18"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dstoyanoff/emotion-primitive.git"
  }
}